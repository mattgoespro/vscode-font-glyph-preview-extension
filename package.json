{
  "name": "iconfont-preview",
  "displayName": "iconfont-preview",
  "description": "preview iconfont file in vscode",
  "version": "0.0.1",
  "icon": "images/logo.png",
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.51.0"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "ttf.preview",
        "displayName": "ttf preview",
        "selector": [
          {
            "filenamePattern": "*.ttf"
          },
          {
            "filenamePattern": "*.otf"
          },
          {
            "filenamePattern": "*.woff"
          },
          {
            "filenamePattern": "*.woff2"
          },
          {
            "filenamePattern": "*.eot"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/opentype.js": "^1.3.1",
    "@types/pako": "^2.0.0",
    "@types/vscode": "^1.51.0",
    "typescript": "^3.3.1"
  },
  "dependencies": {
    "fonteditor-core": "^2.1.11",
    "pako": "^2.1.0"
  }
}
