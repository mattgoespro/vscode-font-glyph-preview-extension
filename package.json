{
  "name": "vscode-font-glyph-preview-extension",
  "displayName": "font-glyph-preview",
  "description": "Preview font glyphs in files from within Visual Studio Code",
  "version": "0.0.1",
  "icon": "assets/logo.png",
  "main": "./dist/extension.js",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.51.0"
  },
  "scripts": {
    "build:dev": "webpack --config webpack.dev.ts",
    "build:prod": "webpack --config webpack.prod.ts",
    "watch": "webpack --config webpack.dev.ts --watch",
    "vscode:prepublish": "npm run build",
    "lint": "eslint ."
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "fontGlyphPreview.editor.preview",
        "displayName": "Font Glyph Preview",
        "selector": [
          {
            "filenamePattern": "*.ttf"
          },
          {
            "filenamePattern": "*.otf"
          },
          {
            "filenamePattern": "*.woff"
          },
          {
            "filenamePattern": "*.woff2"
          },
          {
            "filenamePattern": "*.eot"
          }
        ]
      }
    ],
    "configuration": [
      {
        "properties": {
          "preview-page-size": {
            "type": "number",
            "description": "The number of glyphs to display per page in the preview",
            "default": 10,
            "minimum": 20,
            "maximum": 500,
            "scope": "resource"
          }
        }
      }
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "debug": "^4.3.7",
    "fonteditor-core": "^2.4.1",
    "fontkit": "^2.0.4",
    "pako": "^2.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.8.1",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-transform-react-constant-elements": "^7.25.9",
    "@babel/plugin-transform-react-inline-elements": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@html-eslint/eslint-plugin": "^0.27.0",
    "@html-eslint/parser": "^0.27.0",
    "@types/debug": "^4.1.12",
    "@types/fontkit": "^2.0.7",
    "@types/node": "^22.9.0",
    "@types/opentype.js": "^1.3.8",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.95.0",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-import-resolver-webpack": "^0.13.9",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-loader": "^5.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss-fail-on-warn": "^0.2.1",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.0.9",
    "postcss-scss": "^4.0.9",
    "sass": "^1.80.6",
    "sass-loader": "^16.0.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "webpack": "^5.96.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  }
}
