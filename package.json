{
  "name": "vscode-font-glyph-preview-extension",
  "displayName": "font-glyph-preview",
  "description": "Preview font glyphs in files from within Visual Studio Code",
  "version": "0.0.1",
  "icon": "images/logo.png",
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.51.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "precompile": "npm run clean",
    "watch": "tsc -p . -w",
    "clean": "rm -rf dist/*"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "fontGlyphPreview.editor.preview",
        "displayName": "Font Glyph Preview",
        "selector": [
          {
            "filenamePattern": "*.ttf"
          },
          {
            "filenamePattern": "*.otf"
          },
          {
            "filenamePattern": "*.woff"
          },
          {
            "filenamePattern": "*.woff2"
          },
          {
            "filenamePattern": "*.eot"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "fonteditor-core": "^2.1.11",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/opentype.js": "^1.3.1",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@types/vscode": "^1.88.0",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
