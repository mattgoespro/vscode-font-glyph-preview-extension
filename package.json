{
  "name": "vscode-font-glyph-preview-extension",
  "displayName": "font-glyph-preview",
  "description": "Preview font glyphs in files from within Visual Studio Code",
  "version": "0.0.1",
  "icon": "assets/logo.png",
  "main": "./dist/extension.js",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.51.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.ts --mode development",
    "watch": "webpack --config webpack.config.ts --mode development --watch",
    "vscode:prepublish": "npm run build",
    "lint": "eslint ."
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "fontGlyphPreview.editor.preview",
        "displayName": "Font Glyph Preview",
        "selector": [
          {
            "filenamePattern": "*.ttf"
          },
          {
            "filenamePattern": "*.otf"
          },
          {
            "filenamePattern": "*.woff"
          },
          {
            "filenamePattern": "*.woff2"
          },
          {
            "filenamePattern": "*.eot"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.24.5",
    "fonteditor-core": "^2.4.0",
    "fontkit": "^2.0.2",
    "pako": "^2.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-react-constant-elements": "^7.24.1",
    "@babel/plugin-transform-react-inline-elements": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@html-eslint/eslint-plugin": "^0.24.1",
    "@html-eslint/parser": "^0.24.1",
    "@types/fontkit": "^2.0.7",
    "@types/node": "^20.12.8",
    "@types/opentype.js": "^1.3.8",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.89.0",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.1",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-loader": "^5.0.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-fail-on-warn": "^0.2.1",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.11",
    "postcss-scss": "^4.0.9",
    "sass": "^1.76.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
