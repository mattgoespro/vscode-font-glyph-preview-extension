{
  "name": "vscode-font-glyph-preview-extension",
  "displayName": "font-glyph-preview",
  "description": "Preview font glyphs in files from within Visual Studio Code",
  "version": "0.0.1",
  "icon": "images/logo.png",
  "main": "./dist/extension.js",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.51.0"
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "webpack --config webpack.config.ts --mode development",
    "watch": "webpack --config webpack.config.ts --mode development --watch"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "fontGlyphPreview.editor.preview",
        "displayName": "Font Glyph Preview",
        "selector": [
          {
            "filenamePattern": "*.ttf"
          },
          {
            "filenamePattern": "*.otf"
          },
          {
            "filenamePattern": "*.woff"
          },
          {
            "filenamePattern": "*.woff2"
          },
          {
            "filenamePattern": "*.eot"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.24.4",
    "fonteditor-core": "^2.4.0",
    "fontkit": "^2.0.2",
    "pako": "^2.1.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-react-constant-elements": "^7.24.1",
    "@babel/plugin-transform-react-inline-elements": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@types/fontkit": "^2.0.7",
    "@types/node": "^20.12.7",
    "@types/opentype.js": "^1.3.8",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.88.0",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
